{
    "manifest_version": 2,
    "name": "Epilepsy Blocker",
    "version": "1.0",
    "background": {
      "scripts": ["script.js"],
      "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
      ],
    "permissions": [ "activeTab",
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"]
}
  